{
  "name": "express-web",
  "version": "1.0.0",
  "description": "tdd with express",
  "main": "app.js",
  "dependencies": {
    "express": "^4.18.1"
  },
  "devDependencies": {
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4"
  },
  "scripts": {
    "test": "jest --watch",
    "lint":"eslint . "
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "eqeqeq":"warn",
      "prettier/prettier":"warn"
    }
  },

  "prettier":{
    "singleQuote": false,
    "printWidth":120
   
},


  "author": "skyler",
  "license": "ISC"
}
